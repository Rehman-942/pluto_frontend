# Frontend Environment Configuration Template
# Copy this file to .env and update values for your setup

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api

# Azure Storage Configuration (for media display)
REACT_APP_AZURE_STORAGE_ACCOUNT_NAME=your-storage-account-name
REACT_APP_AZURE_STORAGE_CONTAINER_NAME=your-container-name

# Application Info
REACT_APP_APP_NAME=Pluto
REACT_APP_VERSION=1.0.0
REACT_APP_NODE_ENV=development

# Development Configuration
PORT=3000
GENERATE_SOURCEMAP=true
ESLINT_NO_DEV_ERRORS=true
TSC_COMPILE_ON_ERROR=true
